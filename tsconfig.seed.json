{
    "compilerOptions": {
      "target": "ES2016",
      "module": "CommonJS",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "resolveJsonModule": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": ["prisma/seed.ts", "lib/data/**/*.ts"]
  }